{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "shared.defs.json",
  "$defs": {
    "gameId": { "type": "string", "minLength": 1 },
    "playerId": { "type": "string", "minLength": 1 },
    "username": { "type": "string", "minLength": 1 },
    "address": {
      "description": "Coin address; supports Solana base58 addresses and practice token identifiers like '0xPractice'.",
      "oneOf": [
        { "type": "string", "const": "0xPractice" },
        { "type": "string", "pattern": "^[1-9A-HJ-NP-Za-km-z]{32,44}$" }
      ]
    },
    "tick": { "type": "integer", "minimum": 0 },
    "timestamp_ms": { "type": "integer", "minimum": 0, "maximum": 9007199254740991 },
    "tradeId": { "type": "string", "minLength": 1 },
    "sol": { "type": "number", "minimum": 0 },
    "coin": { "type": "string", "minLength": 1 }
  }
}